<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="ParallelMobileSuite" parallel="tests" thread-count="2">
    <listeners>
        <listener class-name="com.axonvibe.Base.TestListener"/>
    </listeners>
    <test name="AndroidTests">
        <parameter name="platform" value="android"/>
        <classes>
            <class name="com.axonvibe.tests.TestOpenApp"/>
        </classes>
    </test>

    <test name="iOSTests">
        <parameter name="platform" value="ios"/>
        <classes>
            <class name="com.axonvibe.tests.TestOpenApp"/>
        </classes>
    </test>

</suite>
